{
  "runner.bootstrap": "vendor/autoload.php",
  "runner.path": "benchmarks",
  "report.generators": {
    "compare": {
      "generator": "expression",
      "cols": [
        "benchmark",
        "subject",
        "mean",
        "rstdev",
        "mem_peak"
      ]
    },
    "detailed": {
      "generator": "expression",
      "cols": [
        "benchmark",
        "subject",
        "mean",
        "mode",
        "rstdev",
        "stdev",
        "mem_peak",
        "revs"
      ]
    }
  }
}